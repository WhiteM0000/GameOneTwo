socket.on('my_response', function() {console.log('Server: ZZZzzz')});

        var socket = io.connect('http://' + document.domain + ':' + location.port);
            socket.on('connect', function() {socket.emit('my_event', {data: 'I\'m connected!'});alert("Соединение установлено.")});


            socket.on('my_response', function(msg) {
                $('#log').append('<br>' + $('<div/>').text('Received #' + msg.count + ': ' + msg.data).html());
            });
